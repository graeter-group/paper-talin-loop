@article{dasMolecularMechanotransductionPathway2015,
  title = {A Molecular Mechanotransduction Pathway Regulates Collective Migration of Epithelial Cells},
  author = {Das, Tamal and Safferling, Kai and Rausch, Sebastian and Grabe, Niels and Boehm, Heike and Spatz, Joachim P.},
  year = {2015},
