@article{goultStructureDoubleUbiquitinlike2010,
  title = {Structure of a Double Ubiquitin-like Domain in the Talin Head: A Role in Integrin Activation},
  shorttitle = {Structure of a Double Ubiquitin-like Domain in the Talin Head},
  author = {Goult, Benjamin T and Bouaouina, Mohamed and Elliott, Paul R and Bate, Neil and Patel, Bipin and Gingras, Alexandre R and Grossmann, J G{\"u}nter and Roberts, Gordon C K and Calderwood, David A and Critchley, David R and Barsukov, Igor L},
