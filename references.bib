@article{hainingAllSubdomainsTalin2016,
  title = {All {{Subdomains}} of the {{Talin Rod Are Mechanically Vulnerable}} and {{May Contribute To Cellular Mechanosensing}}},
  author = {Haining, Alexander William M. and {von Essen}, Magdal{\'e}na and Attwood, Simon J. and Hyt{\"o}nen, Vesa P. and {del R{\'i}o Hern{\'a}ndez}, Armando},
  year = {2016},
