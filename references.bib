@article{naughtonModesInteractionPleckstrin2018,
  title = {Modes of {{Interaction}} of {{Pleckstrin Homology Domains}} with {{Membranes}}: {{Toward}} a {{Computational Biochemistry}} of {{Membrane Recognition}}},
  shorttitle = {Modes of {{Interaction}} of {{Pleckstrin Homology Domains}} with {{Membranes}}},
  author = {Naughton, Fiona B. and Kalli, Antreas C. and Sansom, Mark S. P.},
  year = {2018},
  month = feb,
  journal = {Journal of Molecular Biology},
  volume = {430},
  number = {3},
  pages = {372--388},
  issn = {0022-2836},
  doi = {10.1016/j.jmb.2017.12.011},
  abstract = {Pleckstrin homology (PH) domains mediate protein\textendash membrane interactions by binding to phosphatidylinositol phosphate (PIP) molecules. The structural and energetic basis of selective PH\textendash PIP interactions is central to understanding many cellular processes, yet the molecular complexities of the PH\textendash PIP interactions are largely unknown. Molecular dynamics simulations using a coarse-grained model enables estimation of free-energy landscapes for the interactions of 12 different PH domains with membranes containing PIP2 or PIP3, allowing us to obtain a detailed molecular energetic understanding of the complexities of the interactions of the PH domains with PIP molecules in membranes. Distinct binding modes, corresponding to different distributions of cationic residues on the PH domain, were observed, involving PIP interactions at either the ``canonical'' (C) and/or ``alternate'' (A) sites. PH domains can be grouped by the relative strength of their C- and A-site interactions, revealing that a higher affinity correlates with increased C-site interactions. These simulations demonstrate that simultaneous binding of multiple PIP molecules by PH domains contributes to high-affinity membrane interactions, informing our understanding of membrane recognition by PH domains in vivo.},
  langid = {english},
  keywords = {Free-energy,Membrane,Molecular dynamics,PH domains,PIP},
  file = {/home/jannik/Zotero/storage/M8T6BQVZ/Naughton et al. - 2018 - Modes of Interaction of Pleckstrin Homology Domain.pdf;/home/jannik/Zotero/storage/V4Z6K9CL/naughton2017.pdf.pdf;/home/jannik/Zotero/storage/VAD7J8VV/naughton2017.pdf.pdf;/home/jannik/Zotero/storage/YLTMNWMU/S0022283617305922.html}
}

